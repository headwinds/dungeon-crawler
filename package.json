{
  "name": "wind-song-hunt3",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.11.1",
    "react": "^16.7.0",
    "react-addons-update": "^15.0.1",
    "react-dom": "^16.7.0",
    "react-redux": "^4.4.5",
    "react-scripts": "2.1.3",
    "redux": "^3.4.0",
    "redux-batched-actions": "^0.1.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.0.1"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar ./src -o ./build",
    "watch-css": "node-sass-chokidar ./src -o ./build --watch",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "start-js": "react-scripts start",
    "now-build": "react-scripts build && mv build dist"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5"
  }
}
